@model List<EntitiyLayer.Concrete.Heading>
@{
    ViewBag.Title = "GetByWriterHeading";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="row">
    <div class="col-md-12">
        <div class="timeline">
            @foreach (var item in Model)
            {<div>
                    <i class="fas fa-comments bg-yellow"></i>
                    <div class="timeline-item">
                        <span class="time"><i class="fas fa-clock"></i>@(((DateTime) item.Date).ToString("D"))</span>
                        <h3 class="timeline-header">   @item.Category.CategoryName KATEGORİSİ <a href="~/Home/GetBy/@item.HeadingID">@item.HeadingName</a> BAŞLIĞI    </h3>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<script src="~/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>
<script src="~/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="~/AdminLTE-3.0.4/dist/js/adminlte.min.js"></script>
<script src="~/AdminLTE-3.0.4/dist/js/demo.js"></script>